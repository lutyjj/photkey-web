<p id="notice"><%= notice %></p>

<h1>Like Logs</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Photo</th>
      <th>Category</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @like_logs.each do |like_log| %>
      <tr>
        <td><%= like_log.user_id %></td>
        <td><%= like_log.photo_id %></td>
        <td><%= like_log.category_id %></td>
        <td><%= link_to 'Show', like_log %></td>
        <td><%= link_to 'Edit', edit_like_log_path(like_log) %></td>
        <td><%= link_to 'Destroy', like_log, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Like Log', new_like_log_path %>
